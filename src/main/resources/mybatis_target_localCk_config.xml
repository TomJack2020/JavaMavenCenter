<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="com.clickhouse.jdbc.ClickHouseDriver"/>
                <property name="url" value="jdbc:clickhouse://localhost:8123/imdb?useSSL=false&amp;rewriteBatchedStatements=true"/>
                <property name="username" value="default"/>
                <property name="password" value="test123"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="TargetMapper.xml"/>
        <mapper resource="TruncateMapper.xml"/>
        <mapper resource="ProductTargetListMapper.xml"/>
    </mappers>
</configuration>