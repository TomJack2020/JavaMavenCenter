<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserMapper">
    <select id="selectAll" resultType="com.itheima.pojo.Product">
        select sku, spu, title_cn from yibai_prod_base.yibai_prod_sku
        limit #{start}, #{size};
    </select>
</mapper>
